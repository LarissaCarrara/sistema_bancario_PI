// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id       Int      @id @default(autoincrement())
  nome     String
  telefone String
  email    String
  cpf      String   @unique
  senha    String
  saldo    Float
  limite   Float
}